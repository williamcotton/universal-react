{
  "name": "universal-react",
  "version": "1.0.0",
  "description": "Universal React",
  "main": "src/js/electron/main.js",
  "engines": {
    "node": "5.5.x",
    "npm": "3.3.x"
  },
  "scripts": {
    "test": "npm run build && tapes test/**/*.js test/**/**/*-spec.js | tap-spec",
    "build": "make",
    "watch": "nodemon -q -x 'npm test'",
    "build-dev": "watchify src/js/browser/index.js -o public/build.js -t [ babelify --presets [ es2015 react ] ] -v -d",
    "build-css-dev": "nodemon -w ./src/scss --ext css,scss --exec make -- build_debug_css",
    "start-dev": "NODE_ENV=development foreman start -f Procfile_dev",
    "db:auth:setup": "./db/migrate.js",
    "db:migrate:currentVersion": "knex -- migrate:currentVersion",
    "db:migrate:latest": "knex -- migrate:latest",
    "db:migrate:make": "knex -- migrate:make",
    "db:migrate:rollback": "knex -- migrate:rollback",
    "lint": "eslint ./src ./test ./migrations",
    "build-electron-dev": "watchify src/js/electron/index.js -o app/build.js -t [ babelify --presets [ es2015 react ] ] -v -d",
    "start-electron": "electron .",
    "postinstall": "make"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/williamcotton/universal-react.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/williamcotton/universal-react/issues"
  },
  "browser": {
    "request": "browser-request"
  },
  "homepage": "https://github.com/williamcotton/universal-react",
  "dependencies": {
    "async": "^1.5.2",
    "babel-cli": "^6.4.5",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babelify": "^7.2.0",
    "bcrypt": "^0.8.5",
    "body-parser": "^1.14.2",
    "bookshelf": "^0.9.1",
    "browser-express": "^2.0.1",
    "browser-request": "^0.3.3",
    "browserify": "^13.0.0",
    "compression": "^1.6.1",
    "cookie-parser": "^1.4.1",
    "cookie-session": "^2.0.0-alpha.1",
    "csurf": "^1.8.3",
    "d3": "^3.5.14",
    "debug": "^2.2.0",
    "ejs": "^2.4.1",
    "express": "^4.13.4",
    "http-errors": "^1.3.1",
    "inflection": "^1.8.0",
    "jsonwebtoken": "^5.5.4",
    "knex": "^0.9.0",
    "moment": "^2.11.1",
    "morgan": "^1.6.1",
    "multer": "^1.1.0",
    "node-jsx": "^0.13.3",
    "node-sass": "^3.4.2",
    "node-uuid": "^1.4.7",
    "nodemon": "^1.8.1",
    "on-headers": "^1.0.1",
    "pg": "^4.4.4",
    "react": "^0.14.7",
    "react-addons-test-utils": "^0.14.7",
    "react-bootstrap": "^0.28.2",
    "react-dom": "^0.14.7",
    "react-faux-dom": "^2.1.1",
    "request": "^2.69.0",
    "retry-promise": "^1.0.0",
    "sendgrid": "^2.0.0",
    "uglify-js": "^2.6.1",
    "validator": "^4.5.2"
  },
  "devDependencies": {
    "babel-eslint": "^4.1.6",
    "babel-preset-react": "^6.3.13",
    "cheerio": "^0.19.0",
    "eslint": "^1.10.3",
    "eslint-config-standard": "^4.4.0",
    "eslint-config-standard-react": "^1.2.1",
    "eslint-plugin-react": "^3.16.1",
    "eslint-plugin-standard": "^1.3.1",
    "jsdom": "^3.1.2",
    "localStorage": "^1.0.3",
    "nedb": "^1.7.1",
    "react-addons-test-utils": "^0.14.6",
    "router": "^1.1.3",
    "tap-spec": "^4.1.0",
    "tapes": "^3.0.0",
    "watchify": "^3.7.0"
  }
}
