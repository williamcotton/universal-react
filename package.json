{
  "name": "universal-react",
  "version": "0.1.1",
  "description": "Universal React",
  "main": "src/js/electron/main.js",
  "scripts": {
    "test": "npm run build && tapes test/**/*.js test/**/**/*-spec.js | tap-spec",
    "build": "make",
    "watch": "nodemon -q -x 'npm test'",
    "build-dev": "watchify src/js/browser/index.js -o public/build.js -t [ babelify --presets [ es2015 react ] ] -v -d",
    "start-dev": "export NODE_ENV=development && foreman start -f Procfile_dev",
    "build-electron-dev": "watchify src/js/electron/index.js -o app/build.js -t [ babelify --presets [ es2015 react ] ] -v -d",
    "start-electron": "electron .",
    "postinstall": "make"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/williamcotton/universal-react.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/williamcotton/universal-react/issues"
  },
  "browser": {
    "request": "browser-request"
  },
  "homepage": "https://github.com/williamcotton/universal-react",
  "dependencies": {
    "babel": "^6.3.26",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babelify": "^7.2.0",
    "bcrypt": "^0.8.5",
    "body-parser": "^1.14.2",
    "browser-express": "^1.2.0",
    "browser-request": "^0.3.3",
    "browserify": "^12.0.1",
    "compression": "^1.5.2",
    "cookie": "^0.2.2",
    "cookie-parser": "^1.4.0",
    "cookie-session": "^2.0.0-alpha.1",
    "csurf": "^1.8.3",
    "ejs": "^2.3.4",
    "express": "^4.12.4",
    "google-images": "^0.1.0",
    "jsonwebtoken": "^5.5.4",
    "nedb": "^1.6.0",
    "node-jsx": "^0.13.3",
    "node-sass": "^3.3.3",
    "nodemon": "^1.7.1",
    "react": "^0.14.6",
    "react-bootstrap": "^0.28.1",
    "react-dom": "^0.14.6",
    "request": "^2.64.0",
    "tap-spec": "^4.1.0",
    "tape": "^4.4.0",
    "tapes": "^3.0.0",
    "uglify-js": "^2.4.23",
    "validator": "^4.5.0",
    "watchify": "^3.4.0"
  },
  "devDependencies": {
    "async": "^1.4.2",
    "cheerio": "^0.19.0",
    "electron-builder": "^2.5.0",
    "electron-prebuilt": "^0.36.2",
    "electron-reload": "^0.2.0",
    "jsdom": "^3.1.2",
    "localStorage": "^1.0.3",
    "nodemon": "^1.3.7",
    "react-addons-test-utils": "^0.14.6",
    "router": "^1.1.3",
    "tap-spec": "^4.0.2",
    "tapes": "^2.0.0",
    "watchify": "^3.2.1",
    "xmlhttprequest": "^1.7.0"
  }
}
