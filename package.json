{
  "name": "universal-react",
  "version": "1.0.0",
  "description": "Universal React",
  "main": "src/js/electron/main.js",
  "engines": {
    "node": "5.6.x",
    "npm": "3.6.x"
  },
  "scripts": {
    "test": "npm run build && tapes test/**/*.js test/**/**/*-spec.js | tap-spec",
    "build": "make",
    "watch": "nodemon -q -x 'npm test'",
    "build-dev": "watchify src/js/browser/index.js -o public/build.js -t [ babelify --presets [ es2015 react ] ] -v -d",
    "build-css-dev": "nodemon -w ./src/scss --ext css,scss --exec make -- build_debug_css",
    "start-dev": "NODE_ENV=development foreman start -f Procfile_dev",
    "db:auth:setup": "./db/migrate.js",
    "db:migrate:currentVersion": "knex -- migrate:currentVersion",
    "db:migrate:latest": "knex -- migrate:latest",
    "db:migrate:make": "knex -- migrate:make",
    "db:migrate:rollback": "knex -- migrate:rollback",
    "lint": "eslint ./src ./test ./migrations",
    "build-electron-dev": "watchify src/js/electron/index.js -o app/build.js -t [ babelify --presets [ es2015 react ] ] -v -d",
    "start-electron": "electron .",
    "postinstall": "make"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/williamcotton/universal-react.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/williamcotton/universal-react/issues"
  },
  "browser": {
    "request": "browser-request"
  },
  "homepage": "https://github.com/williamcotton/universal-react",
  "dependencies": {
    "async": "^2.0.0-rc.5",
    "babel-cli": "^6.8.0",
    "babel-eslint": "^6.0.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.3.0",
    "bcrypt": "^0.8.6",
    "body-parser": "^1.15.1",
    "bookshelf": "^0.9.5",
    "browser-express": "^2.2.0",
    "browser-request": "^0.3.3",
    "browserify": "^13.0.1",
    "cheerio": "^0.20.0",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.1",
    "cookie-session": "^2.0.0-alpha.1",
    "csurf": "^1.8.3",
    "d3": "^3.5.17",
    "debug": "^2.2.0",
    "ejs": "^2.4.1",
    "eslint": "^2.10.2",
    "eslint-config-standard": "^5.3.1",
    "eslint-config-standard-react": "^2.4.0",
    "eslint-plugin-react": "^5.1.1",
    "eslint-plugin-standard": "^1.3.2",
    "express": "^4.13.4",
    "http-errors": "^1.3.1",
    "inflection": "^1.10.0",
    "jsdom": "^9.1.0",
    "jsonwebtoken": "^6.2.0",
    "knex": "^0.11.3",
    "moment": "^2.13.0",
    "morgan": "^1.7.0",
    "multer": "^1.1.0",
    "nedb": "^1.8.0",
    "node-jsx": "^0.13.3",
    "node-sass": "^3.7.0",
    "node-uuid": "^1.4.7",
    "nodemon": "^1.9.2",
    "on-headers": "^1.0.1",
    "pg": "^4.5.5",
    "react": "^15.0.2",
    "react-addons-test-utils": "^15.0.2",
    "react-bootstrap": "^0.29.4",
    "react-dom": "^15.0.2",
    "react-faux-dom": "^2.5.0",
    "recorderjs": "^1.0.1",
    "request": "^2.72.0",
    "retry-promise": "^1.0.0",
    "sendgrid": "^2.0.0",
    "uglify-js": "^2.6.2",
    "validator": "^5.2.0",
    "webrtc-adapter": "^1.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^4.1.6",
    "babel-preset-react": "^6.3.13",
    "cheerio": "^0.19.0",
    "electron-reload": "^0.3.0",
    "eslint": "^1.10.3",
    "eslint-config-standard": "^4.4.0",
    "eslint-config-standard-react": "^1.2.1",
    "eslint-plugin-react": "^3.16.1",
    "eslint-plugin-standard": "^1.3.1",
    "jsdom": "^3.1.2",
    "localStorage": "^1.0.3",
    "nedb": "^1.7.1",
    "react-addons-test-utils": "^0.14.6",
    "router": "^1.1.3",
    "tap-spec": "^4.1.0",
    "tapes": "^3.0.0",
    "watchify": "^3.7.0"
  }
}
